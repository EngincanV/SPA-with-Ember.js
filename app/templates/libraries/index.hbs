<div class="container">
    <h3>List</h3>
    <br/>
    {{#each model as |library|}}
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">{{library.name}}</h3>
            </div>
            <div class="panel-body">
                <p>Address: {{library.address}}</p>
                <p>Phone: {{library.phone}}</p>
            </div>
        </div>
    {{/each}}
</div>